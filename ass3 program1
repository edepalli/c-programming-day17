//print lowest numbers using while loop
#include<stdio.h>
int main()
{ 
  int  i,n,ele,l=0;
  printf("enter number limit:");
  scanf("%d",&n);
  printf("enter elements:");
  i=1;
  while(i<=n)
  { 
     scanf("%d",&ele);
    if(i==1)
	  l=ele;
    else if(l>=ele)
      l=ele;
      i++;
  }
  printf("\nlowest=%d",l);
}
